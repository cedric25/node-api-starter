generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  user_id    String   @id
  email      String   @unique(map: "users.email_unique")
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("users")
}
